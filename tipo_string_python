nome = 'Geek University'

# [::-1] -> Comece do primeiro elemento, va até o ultimo elemento inverta
 
print(nome[::-1]) # Inversão da String Pythonico

print(nome.replace('G', 'P'))

print(Type(nome))

texto = 'socorram me subino onibus em marrocos' # Palindromo
print(texto)

print(texto[::-1])

nome = 'ana' #Palindromo
